    <mj-section padding="0">
      <mj-include
        path="./partials/components/component-section-title-stories.mjml" />
    </mj-section>

    <!-- Body Content Blocks 1 -->
    <mj-section padding="0" css-class="light-section">
      <mj-include
        path="./partials/blocks/block-bg-white-image-left-h2-text-button-blue-left.mjml" />
    </mj-section>

    <!-- Body Content Blocks 2 -->
    <mj-section padding="0" css-class="light-section light-border">
      <mj-include
        path="./partials/blocks/block-bg-gold-image-left-h2-text-button-blue-left.mjml" />
