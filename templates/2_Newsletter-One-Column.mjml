    </mj-section>

    <!-- Body Content Blocks 5 -->
    <mj-section padding="0" css-class="light-section light-border">
      <mj-include
        path="./partials/blocks/block-bg-white-image-left-h2-text-button-blue-left.mjml" />
    </mj-section>

    <!-- Body Content Blocks 6 -->
    <mj-section padding="0" css-class="light-section light-border">
      <mj-include
        path="./partials/blocks/block-bg-gold-image-left-h2-text-button-blue-left.mjml" />
    </mj-section>

    <!-- Body Content Blocks 7 -->
    <mj-section padding="0" css-class="light-section light-border">
      <mj-include
        path="./partials/blocks/block-bg-white-image-left-h2-text-button-blue-left.mjml" />
    </mj-section>

    <!-- Body Content Blocks 8 -->
    <mj-section padding="0" css-class="light-section light-border">
      <mj-include
        path="./partials/blocks/block-bg-gold-image-left-h2-text-button-blue-left.mjml" />
    </mj-section>

    <!-- Section Title Bar -->
    <mj-section padding="0">
      <mj-include
        path="./partials/components/component-section-title-events.mjml" />
    </mj-section>

    <!-- Events Block - 2 Column Layout -->
    <mj-section padding="0">
      <mj-include
        path="./partials/blocks/block-events-2column-button-gold-center.mjml" />
    </mj-section>

    <!-- Footer Section -->
    <mj-section padding="0">
      <!-- Social Media Footer -->
      <mj-include path="./partials/footers/social/footer-social-32.mjml" />
    </mj-section>

    <mj-section padding="0">
      <!-- Contact Footer -->
      <mj-include path="./partials/footers/footer-primary.mjml" />
    </mj-section>
  </mj-body>
</mjml>
