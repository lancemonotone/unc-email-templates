    <!-- Body Content Blocks 3 -->
    <mj-section padding="0" css-class="light-section light-border">
      <mj-include
        path="./partials/blocks/block-bg-white-image-left-h2-text-button-blue-left.mjml" />
    </mj-section>

    <!-- Body Content Blocks 4 -->
    <mj-section padding="0" css-class="light-section light-border">
      <mj-include
